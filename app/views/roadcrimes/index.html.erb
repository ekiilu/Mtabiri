<h1>Showing all RoadCrimes</h1>

<table class="table_list">
  <tr>
    <th>Nationalid</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @roadcrimes.each do |roadcrime| %>
  <tr>
    <td><%= roadcrime.nationalid %></td>
    <td><%= roadcrime.location %></td>
    <td><%= link_to 'Show', roadcrime %></td>
    <td><%= link_to 'Edit', edit_roadcrime_path(roadcrime) %></td>
    <td><%= link_to 'Destroy', roadcrime, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

